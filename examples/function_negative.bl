// overloading
fun o_load(x: Int) ->Int{
return x * 10;
}

// overloading is not supported
fun o_load(x: String) -> String{
    return x;
}

print(o_load(1));
print(o_load("Hello"));


// No operation allowed for Any type
fun test(x:Any)->Unit{
    print(x + 2);
}

test(1);
test("Hello");